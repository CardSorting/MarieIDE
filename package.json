{
  "name": "marieide",
  "version": "0.1.0",
  "description": "AI-First Development Environment - Autonomous AI-driven IDE",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json && electron dist/main.js --dev",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "test": "jest",
    "test:ai": "jest --testPathPattern=ai",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,json}",
    "setup:ai": "node scripts/setup-ai-models.js",
    "benchmark:ai": "node scripts/benchmark-ai-performance.js"
  },
  "keywords": [
    "ide",
    "ai",
    "development",
    "autonomous",
    "code-generation",
    "ai-first"
  ],
  "author": "MarieIDE Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^8.2.2",
    "electron": "^27.1.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.7.1",
    "@google/generative-ai": "^0.2.1",
    "@monaco-editor/react": "^4.6.0",
    "@openai/api": "^4.20.1",
    "axios": "^1.6.2",
    "electron-store": "^8.1.0",
    "monaco-editor": "^0.44.0",
    "ollama": "^0.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
